{
    "name": "brackets/admin-translations",
    "description": "Translation manager for brackets/admin-ui",
    "keywords": [
        "translations",
        "localizations",
        "manager",
        "admin",
        "cms",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Pavol Perdík",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "David Běhal",
            "email": "david.behal@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "Richard Dominik",
            "email": "richard.dominik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "develop-algoritma/admin-ui": "dev-master",
        "develop-algoritma/admin-listing": "dev-master",
        "develop-algoritma/translatable": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "~4.0|~5.0|~6.0|~7.0|~8.0|~9.0",
        "phpunit/phpunit": "^8.5|^9.0|^10.0|^11.0",
        "doctrine/dbal": "~2.5.13|^2.6|^3.5|~4.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Brackets\\AdminTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brackets\\AdminTranslations\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brackets\\AdminTranslations\\AdminTranslationsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/develop-algoritma/admin-ui.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/develop-algoritma/admin-listing.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/develop-algoritma/translatable.git"
        }
        ]
    
}
